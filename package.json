{
  "name": "kushki-sns-gateway",
  "version": "0.0.11",
  "description": "Kushki SNS Gateway for managing AWS events.",
  "main": "dist/KushkiSnsGateway.js",
  "typings": "src/KushkiSnsGateway",
  "scripts": {
    "test": "yarn run test:unit && yarn run test:integration",
    "test:unit": "--compilers ts:ts-node/register -r tsconfig-paths/register --ignore *.js",
    "test:integration": "webpack --env.ksh_test integration && ./shell/sls_integration_test.sh",
    "yarn:install": "yarn install && chmod -R 644 node_modules",
    "tsc:typing": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kushki/kushki-sns-gateway.git"
  },
  "keywords": [
    "SNS",
    "AWS",
    "gateway",
    "typescript"
  ],
  "author": "Kushki Dev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kushki/kushki-sns-gateway/issues"
  },
  "homepage": "https://github.com/Kushki/kushki-sns-gateway#readme",
  "devDependencies": {
    "aws-sdk-mock": "^1.7.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "json-schema-faker": "^0.5.0-rc3",
    "mocha": "^3.4.2",
    "tsconfig-paths": "^2.1.2",
    "tslint": "^5.3.2",
    "tslint-eslint-rules": "^4.1.0",
    "tslint-microsoft-contrib": "^5.0.0",
    "typescript": "^2.3.3",
    "typings": "^2.1.1"
  },
  "dependencies": {
    "ajv": "^5.1.3",
    "aws-sdk": "^2.57.0",
    "ts-node": "^3.0.4"
  }
}
